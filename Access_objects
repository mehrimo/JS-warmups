var dogs = {'sparky': 1, 'zion':3, 'henry':5, 'lucky':8};


//write a function to take the dog ages * 7.
function changesAges(dogs) {

  for(var key in dogs) {
  dogs[key] *= 7;
  }

return dogs;
}
console.log(changesAges(dogs));
